<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Version>1.0.0</Version>
    <Authors>PstToolkit Team</Authors>
    <Description>A C# library for reading, creating, and transferring email messages between PST files without using Outlook Interop or paid libraries</Description>
    <PackageTags>PST;Outlook;Email;Parser</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MimeKit" Version="4.0.0" />
  </ItemGroup>

</Project>
